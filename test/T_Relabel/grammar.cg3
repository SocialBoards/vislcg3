DELIMITERS = "<$.>" ;

LIST N = N;
LIST NP = (N Prop) ;
LIST NPAdv = (N Prop) Adv ;
LIST NPAdvV = (N Prop) Adv V ;
LIST NPAdvVX = (N Prop) Adv V X ;

LIST nochangehere = X Y Z ;
LIST norhere = X Y ;

SET N_NO_NP = N - (Prop);

REMOVE NP IF (-1 (Det));
REMOVE (X) IF (-1 nochangehere LINK -1 norhere);
